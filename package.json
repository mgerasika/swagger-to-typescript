{
    "name": "swagger-to-typescript2",
    "version": "0.1.27",
    "private": false,
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "types": "dist/index.d.ts",
    "dependencies": {
        "axios": "^1.3.4"
    },
    "devDependencies": {
        "jest": "^29.5.0",
        "@babel/node": "^7.6.1",
        "@babel/preset-env": "^7.16.0",
        "@babel/preset-typescript": "^7.21.0",
        "@types/jest": "^29.5.0",
        "@types/node": "^14.14.31",
        "@typescript-eslint/eslint-plugin": "^4.28.4",
        "@typescript-eslint/experimental-utils": "^4.28.4",
        "@typescript-eslint/parser": "^4.28.4",
        "eslint": "7.32.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-cypress": "^2.11.3",
        "eslint-plugin-hooks-sort": "^0.1.1",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-no-only-tests": "^2.6.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-unicorn": "^35.0.0",
        "husky": "^7.0.0",
        "stylelint": "^13.12.0",
        "stylelint-order": "^4.1.0",
        "ts-loader": "^8.0.1",
        "rimraf": "^4.4.1",
        "typescript": "4.3.2",
        "lint-staged": "^11.1.1",
        "prettier": "2.3.2",
        "prettier-plugin-organize-imports": "^1.1.1"
    },
    "scripts": {
        "build": "rimraf /dist && tsc",
        "stylelint": "stylelint **/*.scss",
        "stylelint:fix": "stylelint **/*.scss --fix",
        "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}'",
        "lint:fix": "tsc --noEmit && eslint --fix 'src/**/*.{js,ts,tsx}'",
        "test": "jest --config=jest.config.js",
	  	"prettier": "prettier --config ./.prettierrc.js --check \"src/**/*{.ts,.tsx,*.js,*.json.*.babelrc}\" ",
        "prettier:fix": "npm run prettier -- --write"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{ts,tsx,js}": [
            "npm run lint:fix",
            "npm run prettier:fix",
            "git add ."
        ]
    },
    "author": "Mykhaylo Gerasika <mgerasika@gmail.com>",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mgerasika/swagger-to-typescript.git"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/mgerasika/swagger-to-typescript/issues"
    }
}